{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Basic example for react server side rendering",
  "main": "index.js",
  "scripts": {
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:run-server": "nodemon dist/server.js --watch dist",
    "dev": "npm-run-all --parallel dev:*",
    "lint-check": "eslint server/** src/**",
    "lint": "esw webpack.* src --color",
    "lint-watch": "npm run lint -- --watch",
    "prod:build-client": "webpack --config webpack.client.prod.js",
    "prod:build-server": "webpack --config webpack.server.prod.js",
    "build": "npm-run-all --parallel prod:*",
    "start": "node dist/server.js"
  },
  "author": "Rishabh Mishra",
  "license": "MIT",
  "dependencies": {
    "express": "4.16.2",
    "import-export": "1.0.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-css-modules-transform": "1.2.7",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.7",
    "eslint": "4.8.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-watch": "3.1.2",
    "ignore-styles": "5.0.1",
    "node-sass": "4.6.0",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "webpack": "3.6.0",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
